                        "step": 600, 
                        "refId": "A", 
                        "format": "time_series"
                     }
                  ], 
                  "title": "RAM Utilization"
               }, 
               {
                  "datasource": "Prometheus", 
                  "targets": [
                     {
                        "format": "time_series", 
                        "expr": "sum(irate(collectd_interface_if_octets_rx_total{interface!=\"lo\", exported_instance=\"$rgw_servers.$domain\"}[5m]))", 
                        "step": 20, 
                        "legendFormat": "rx", 
                        "intervalFactor": 2, 
                        "refId": "A"
                     }, 
                     {
                        "format": "time_series", 
                        "expr": "sum(irate(collectd_interface_if_octets_tx_total{interface!=\"lo\", exported_instance=\"$rgw_servers.$domain\"}[5m]))", 
